{
  "name": "node-ts-server",
  "version": "1.0.0",
  "scripts": {
    "build": "yarn run tsc:check && rm -rf ./build && NODE_ENV=production webpack",
    "dev": "yarn run tsc:check && NODE_ENV=development webpack",
    "start": "yarn run dev && node build/main.js",
    "dev:server": "nodemon --exec node build/main.js",
    "tsc:check": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@types/express": "^4.17.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mysql2": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.20"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@types/body-parser": "^1.17.1",
    "@types/node": "^12.12.8",
    "babel-loader": "^8.0.6",
    "nodemon": "^1.19.4",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-node-externals": "^1.7.2"
  }
}
